{
  "name": "artsy-palette-docs",
  "description": "Artsy's design system",
  "version": "0.1.0",
  "scripts": {
    "build": "gatsby build",
    "clean": "rm -rf .cache && yarn start",
    "develop": "nodemon --exec \"gatsby develop\"",
    "debug": "nodemon --exec \"node --inspect-brk --no-lazy node_modules/.bin/gatsby develop\" ",
    "emit-graphql-types": "apollo codegen:generate --tagName=graphql --target=typescript __generated__",
    "lint": "tslint -c tslint.json --project tsconfig.json --fix",
    "prettier-write": "prettier --write \"src/**/*.js\"",
    "release": "auto shipit",
    "start": "yarn develop",
    "test": "echo \"Write tests! -> https://gatsby.app/unit-testing\"",
    "type-check": "tsc --pretty --noEmit"
  },
  "apollo": {
    "client": {
      "service": {
        "name": "gatsby",
        "url": "http://localhost:8000/___graphql"
      }
    }
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "pre-push": "yarn run type-check"
    }
  },
  "lint-staged": {
    "*.@(ts|tsx)": [
      "yarn lint",
      "yarn prettier-write",
      "git add"
    ]
  },
  "nodemonConfig": {
    "ignore": [
      ".cache/*",
      "public/*",
      "src/*"
    ],
    "delay": "1500"
  },
  "prettier": {
    "bracketSpacing": true,
    "proseWrap": "always",
    "semi": false,
    "singleQuote": false,
    "trailingComma": "es5"
  },
  "resolutions": {
    "react": "^16.8.0-alpha.1",
    "react-dom": "^16.8.0-alpha.1",
    "@types/react": "16.7.20"
  },
  "devDependencies": {
    "@types/lodash": "^4.14.120",
    "@types/react": "^16.7.20",
    "@types/styled-components": "^3.0.0",
    "apollo": "^2.1.9",
    "auto-release-cli": "2.3.0",
    "gatsby-plugin-typescript": "^2.0.3",
    "husky": "^1.3.1",
    "lint-staged": "^8.1.0",
    "multer": "^1.4.1",
    "netlify-cms-backend-fs": "^0.3.3",
    "nodemon": "^1.18.9",
    "prettier": "^1.15.3",
    "tslint": "^5.12.1",
    "tslint-config-prettier": "^1.17.0",
    "tslint-react": "^3.6.0",
    "typescript": "^3.0.3",
    "typescript-styled-plugin": "^0.13.0",
    "webpack-notifier": "^1.7.0",
    "webpack-shell-plugin": "^0.5.0"
  },
  "dependencies": {
    "@artsy/palette": "2.34.1",
    "@babel/core": "^7.2.2",
    "@mdx-js/mdx": "^0.16.6",
    "@mdx-js/tag": "^0.16.6",
    "@reach/router": "^1.2.1",
    "babel-plugin-styled-components": "^1.10.0",
    "d3-ease": "^1.0.5",
    "docz-utils": "^0.13.6",
    "gatsby": "^2.0.76",
    "gatsby-image": "^2.0.20",
    "gatsby-mdx": "^0.3.4",
    "gatsby-plugin-manifest": "^2.0.9",
    "gatsby-plugin-netlify-cms": "^3.0.10",
    "gatsby-plugin-offline": "^2.0.16",
    "gatsby-plugin-sharp": "^2.0.14",
    "gatsby-plugin-styled-components": "^3.0.4",
    "gatsby-source-filesystem": "^2.0.16",
    "gatsby-transformer-sharp": "^2.1.8",
    "hast-util-to-string": "^1.0.1",
    "lodash": "^4.17.11",
    "marked": "^0.6.0",
    "netlify-cms": "^2.3.3",
    "netlify-cms-widget-mdx": "^0.3.1",
    "prismjs": "^1.15.0",
    "prop-types": "^15.6.2",
    "react": "^16.8.0-alpha.1",
    "react-dom": "^16.8.0-alpha.1",
    "react-head": "^3.0.2",
    "react-powerplug": "^1.0.0",
    "styled-components": "^3.4.5",
    "unstated": "^2.1.1"
  },
  "license": "MIT",
  "publishConfig": {
    "registry": "https://registry.npmjs.org/"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/gatsbyjs/gatsby-starter-default"
  },
  "bugs": {
    "url": "https://github.com/gatsbyjs/gatsby/issues"
  },
  "authors": [
    "Christopher Pappas <damassi.pappas@gmail.com",
    "Art.sy Inc"
  ],
  "keywords": [
    "palette",
    "artsy",
    "design system",
    "styled-system",
    "styled-components",
    "typescript"
  ]
}
